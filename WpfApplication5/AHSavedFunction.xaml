<UserControl x:Class="VSG.AHSavedFunction"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:VSG"
             mc:Ignorable="d" 
             d:DesignWidth="300" Height="25">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" MaxWidth="250"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Label Grid.Column="0" Name="AHFunctionNamex" Margin="0,2,5,2" Content="{Binding Path=AHFunctionName}" HorizontalAlignment="Left" MouseDown="AHFunctionNamex_MouseDown"></Label>
        <Button Grid.Column="1" Margin="0,2,5,2" Name="AHRemoveFunction" Click="AHRemoveFunction_Click">Delete</Button>
    </Grid>
</UserControl>
